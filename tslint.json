{
  "extends": "gts/tslint.json",
  "rules": {
    "no-console": [true],
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "named-imports-order": "lowercase-last"
      }
    ],
    "typedef": [true, "call-signature", "parameter", "property-declaration"]
  },
  "linterOptions": {
    "exclude": ["**/*.json"]
  }
}
